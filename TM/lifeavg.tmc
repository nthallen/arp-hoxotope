double OHLife; Invalidate OHLife;
{ OHLife = (SOCAGc_OOn - SOCAGc_OOff)/(SOCFGc_OOn-SOCFGc_OOff);
  Validate OHLife;
}

double LABLife; Invalidate LABLife;
{ LABLife = (LABAGc_LOn - LABAGc_LOff)/(LABFGc_LOn-LABFGc_LOff);
  Validate LABLife;
}

double N2Life; Invalidate N2Life;
{ N2Life = (LABAGc_N/LABFGc_N);
  Validate N2Life;
}
