Experiment=HOxP
HomeDir=/home/hoxp
FixDisk=yes
AltDir=/pc/home/hoxp
DiskID="POLARIS Remote Archive"
RemoteHost=$abby

case "$reduce" in
  *lifetime*)
	Extractions=lifeext
	Analysis="$Analysis"'anal/lifeoh $analdir;';;
esac
case "$reduce" in
  *scans*)
	Extractions="$Extractions 'peakext -p'"
	Analysis="$Analysis"'anal/rtgscans $analdir;';;
esac
